{
  "name": "node-red-contrib-modbus",
  "version": "6.0.0-beta.1",
  "private": false,
  "description": "The all in one Modbus TCP, UDP and Serial contribution long term supported package for Node-RED.",
  "dependencies": {
    "@openp4nr/node-modbus": "/Users/biancode/Development/P4NR/modbus/node-modbus-serial",
    "@xstate/fsm": "~2.1.0",
    "bson": "~6.10.1",
    "debug": "~4.4.0",
    "jsmodbus": "~4.0.10",
    "line-by-line": "~0.1.6",
    "source-map-support": "~0.5.21",
    "underscore": "~1.13.7"
  },
  "optionalDependencies": {
    "@serialport/list": "~12.0.0",
    "serialport": "~12.0.0"
  },
  "keywords": [
    "node-red",
    "plusfornodered",
    "modbus",
    "automation",
    "biancoroyal"
  ],
  "engines": {
    "node": ">=18.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/biancoroyal/node-red-contrib-modbus.git"
  },
  "node-red": {
    "version": ">=3",
    "nodes": {
      "Modbus-Client": "modbus/modbus-client.js",
      "Modbus-Response": "modbus/modbus-response.js",
      "Modbus-Read": "modbus/modbus-read.js",
      "Modbus-Getter": "modbus/modbus-getter.js",
      "Modbus-Flex-Getter": "modbus/modbus-flex-getter.js",
      "Modbus-Write": "modbus/modbus-write.js",
      "Modbus-Flex-Write": "modbus/modbus-flex-write.js",
      "Modbus-Server": "modbus/modbus-server.js",
      "Modbus-Queue-Info": "modbus/modbus-queue-info.js",
      "Modbus-Flex-Connector": "modbus/modbus-flex-connector.js",
      "Modbus-IO-Config": "modbus/modbus-io-config.js",
      "Modbus-Response-Filter": "modbus/modbus-response-filter.js",
      "Modbus-Flex-Sequencer": "modbus/modbus-flex-sequencer.js",
      "Modbus-Flex-Fc": "modbus/modbus-flex-fc.js"
    }
  },
  "author": "Klaus Landsdorf <klaus.landsdorf@bianco-royal.de>",
  "license": "BSD-3-Clause",
  "main": "node",
  "files": [
    "docs",
    "examples",
    "modbus",
    "extras"
  ],
  "bugs": {
    "url": "https://github.com/biancoroyal/node-red-contrib-modbus/issues/"
  },
  "homepage": "https://plus4nodered.com/",
  "scripts": {
    "mocha:base": "mocha ./test --reporter dot --timeout 6000",
    "mocha:parallel": "npm run mocha:base -- --parallel --recursive",
    "mocha:slow": "npm run mocha:base -- --recursive --timeout 12000",
    "nyc:base": "nyc --reporter=html --reporter=text",
    "prereq": "npm run lint",
    "lint": "standard --fix",
    "clean": "gulp clean && npm run lint",
    "build": "npm run lint && gulp",
    "changelog": "gulp changelog",
    "test": "npm run prereq && npm run mocha:parallel",
    "test:jest": "jest",
    "test:slow": "npm run prereq && npm run mocha:slow",
    "test:verbose": "npm run prereq && mocha ./test --recursive",
    "test:withStop": "npm run prereq && mocha ./test --recursive --bail",
    "test:publish": "npm run clean && npm run prepublishOnly",
    "test:e2e": "npm run prereq && mocha './test/units/modbus-write-e2e-test.js' --parallel --recursive --reporter dot --timeout 100000",
    "test:units": "npm run prereq && mocha './test/units/modbus-client-test.js' --parallel --recursive --reporter dot",
    "test:core": "npm run prereq && mocha './test/core/*-test.js' --parallel --recursive",
    "coverage:base": "npm run prereq && npm run nyc:base -- npm run mocha:parallel",
    "coverage": "npm run coverage:base",
    "coverage:codacy": "nyc --reporter=lcovonly mocha --recursive -R spec && cat ./coverage/lcov.info | codacy-coverage --token $CODACY_COVERAGE_TOKEN && rm -rf ./coverage",
    "coverage:coveralls": "npm run lint && nyc report --reporter=text-lcov | coveralls",
    "prepublishOnly": "npm run clean && npm run changelog && npm test && npm run coverage && npm run build",
    "ci-publish": "ci-publish",
    "release": "standard-version -a",
    "release:beta": "standard-version --prerelease beta",
    "release:alpha": "standard-version --prerelease alpha",
    "dev:link": "npm i && npm run build && npm link",
    "dev:unlink": "npm unlink node-red-contrib-modbus -g",
    "dev:link-local": "npm run build && npm link"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.5",
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@babel/register": "^7.24.5",
    "@node-red/nodes": "^4.1.0",
    "address": "^2.0.1",
    "chai": "^3.5.0",
    "codacy-coverage": "^3.4.0",
    "conventional-changelog-cli": "^5.0.0",
    "coveralls": "^3.1.1",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-conventional-changelog": "^5.0.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-jsdoc3": "^3.0.0",
    "gulp-replace": "^1.1.4",
    "gulp-sequence": "^1.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-uglify": "^3.0.2",
    "jasmine-node": "^3.0.0",
    "jest": "^29.7.0",
    "js-beautify": "^1.15.1",
    "mocha": "^10.4.0",
    "nock": "^13.5.4",
    "node-red": "^4.1.0",
    "node-red-node-test-helper": "^0.3.4",
    "nyc": "^15.1.0",
    "pump": "^3.0.0",
    "should": "^13.2.3",
    "sinon": "^17.0.1",
    "standard": "^17.1.0",
    "standard-version": "^9.5.0",
    "supertest": "^7.0.0",
    "uglify-js": "^3.17.4",
    "uglify-js-harmony": "^2.7.7",
    "when": "^3.7.8"
  },
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "globals": [
      "chai",
      "expect",
      "should",
      "assert",
      "sinon"
    ],
    "ignore": [
      "code/",
      "coverage/",
      "node_modules/",
      "examples/",
      "modbus/",
      "docs",
      "extras",
      "test/resources/**",
      "test/e2e/flows/**",
      "test/integrations/flows/**",
      "test/units/flows/**",
      "__tests__/units/flows/**",
      "__tests__/units/**"
    ]
  },
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/node-red-contrib-modbus"
  }
}
